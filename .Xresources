! =========================
! ======== FONTS ==========
! =========================

Xft.dpi:            96
Xft.antialias:      true
Xft.hinting:        true
Xft.rgba:           rgb
Xft.autohint:       false
Xft.hintstyle:      hintfull
Xft.lcdfilter:      lcddefault


! =========================
! ========= ROFI ==========
! =========================

rofi.terminal:      termite
rofi.ssh-client:    ssh
rofi.width:         30


